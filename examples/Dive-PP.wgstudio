    {
      "id": "7f9671b3-3e84-47d1-8c53-006387106fe2",
      "name": "Execution",
      "workgroup": {
        "x": 8,
        "y": 8,
        "z": 1
      },
      "code": "@compute @workgroup_size(1, 1, 1)\nfn Execution(@builtin(global_invocation_id) gid : vec3<u32>) {\n    let index = gid.y * 64u + gid.x;\n\n\tvar duration: f32 \t\t\t= 120.0 ;\n\tvar max_points: f32 \t\t= duration / dt ;\n\t// var time_dist: u32 \t\t= int(floor(max_points / 1360.0)) ;\n\tvar half_pressure: f32 \t= 75112.41*1.5; //TODO change to (pression init + pression final)/2\n\n\ttime = dt * f32(step) ;\n\n\tif ( m0 [ pp_N2_ti ] < half_pressure ) {\n\t\tone_step_mono() ; // Simulation of one step\n\t} else {\n\t\tm1 [ 63 ] = 0xFFFFFFFF ;\n\t}\n\n}\n"
    },
